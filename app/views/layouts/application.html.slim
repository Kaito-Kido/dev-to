doctype html
html
  head
    title
      | DevTo
    meta[name="viewport" content="width=device-width,initial-scale=1"]
    = csrf_meta_tags
    = csp_meta_tag
    = stylesheet_pack_tag 'application', meida: "all", 'data-turbo-track': 'reload'
    = javascript_pack_tag 'application', 'data-turbo-track': 'reload'
  body
    byebug
    = render "header/header", notifications: @notifications
    = yield
